<%- include ./partials/header.ejs %>
<div class="container" style="padding-top:30">
<div class="row">
    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-3 col-lg-offset-3 toppad" >

      <div class="panel panel-info">
        <div class="panel-heading">
          <h3 class="panel-title">Welcome <%=data[0].first_name%> <%=data[0].last_name%></h3>
        </div>
        <div class="panel-body">
          <div class="row">
            <div class="col-md-3 col-lg-3 " align="center"> <img height="100px" alt="User Pic" src="https://www.w3schools.com/howto/img_avatar.png" class="img-circle img-responsive"> </div>
            <div class=" col-md-9 col-lg-9 "> 
              <table class="table table-user-information">
                <tbody>
                  <tr>
                    <td>User Name:</td>
                    <td><%=data[0].username%></td>
                  </tr>
                  <tr>
                    <td>Mobile:</td>
                    <td><%=data[0].phone_number%></td>
                  </tr>
                  <tr>
                    <td>Address:</td>
                    <td><%=data[0].address%></td>
                  </tr>
                  <tr>
                    <td>Email:</td>
                    <td><%=data[0].address%></td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
        </div>
    </div>
<div>
</div>
<br><br><br><br><br><br><br><br>
 <%- include ./partials/footer.ejs %>
 </div>